{
    "name": "byanelli/support",
    "description": "Support package with interfaces allowing my other packages to interoperate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bill Yanelli",
            "email": "bill.yanelli@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "psy/psysh": "^0.9.9",
        "illuminate/support": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "BYanelli\\Support\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BYanelli\\Support\\Tests\\" : "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
